{
  "expo": {
    "name": "BBT Africa Connect",
    "slug": "radharadhanath",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/book1.png",
    "scheme": "radharadhanath",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "./index.js",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bbtafrica.connect",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/book1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/book1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/book1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "512d8cbb-60ae-44c6-91fd-081ddb9aaace"
      }
    },
    "platforms": ["ios", "android", "web"]
  }
}
